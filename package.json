{
  "name": "create-react-app-with-eel",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "antd": "^5.21.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "generate:jsonschema": "python3 ./scripts/gene_jsonschema.py -o ./public/schemas",
    "generate:types": "json2ts --cwd ./public/schemas -i ./public/schemas -o ./src/types --declareExternallyRerefenced false && python3 ./scripts/ts2js.py -p ./src/types",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "start:eel": "python3 doctec/index.py --develop",
    "build:eel": "python3 -m eel doctec/index.py build --onefile --noconsole --name react-eel-app",
    "clean:build1": "if exist build rd /s /q build",
    "clean:build2": "if exist dist rd /s /q dist",
    "start": "npm-run-all -p generate:* start:*",
    "build": "npm-run-all clean:build1 clean:build2 generate:* build:*",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "antd-style": "^3.7.0",
    "json-schema-to-typescript": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.14"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
